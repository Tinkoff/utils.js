{
  "git": {
    "commitMessage": "chore(release): version ${version}"
  },
  "github": {
    "release": true
  },
  "npm": {
    "publishPath": "./build"
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "conventionalcommits",
      "infile": "CHANGELOG.md"
    }
  }
}
